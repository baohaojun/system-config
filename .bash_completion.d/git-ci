# jira(1) completion


_git_ci()
{
    local cmd cur prev skinsdir IFS=$' \t\n' i j k=0

    COMPREPLY=()
    cmd=${COMP_WORDS[0]}
    _get_comp_words_by_ref cur prev

    case $COMP_CWORD in
        *)
            _jira_file_list ~/jira-users/
            return 0
            ;;
    esac

    return 0
}


complete -o filenames -F _git_ci gitci gerrit-review gerrit-push-review gerrit-add-reviewer gerrit-push-review-smartisan
complete -o filenames -F _git_ci gitci-amend

