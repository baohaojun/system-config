#!/bin/bash


me=$(readlink -f $0)
mydir=$(dirname $me)
cd $mydir

for x in $(
              cat <<EOF

build-essential
ccache
command-not-found
expect
lib32stdc++6
lib32z1
libc6-dev-i386
libc6-i386
libstring-shellquote-perl
openjdk-7-jdk
schedtool
strace
xmlstarlet
zlib1g-dev
zip
libxml2:i386
libxml2-utils
genisoimage
gperf
bison
xorriso
flex
python-mysqldb
htop
mysql-client
libstring-approx-perl
libtext-glob-perl
libdatetime-perl
python3
ant
less
curl
openssh-client
subversion
EOF
          ); do
    test -e $x || touch $x
done
echo ok

