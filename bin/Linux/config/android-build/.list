#!/bin/bash


me=$(readlink -f $0)
mydir=$(dirname $me)
cd $mydir

for x in $(
              cat <<EOF
ant
bison
build-essential
ccache
command-not-found
curl
daemon
expect
flex
genisoimage
gitk
gperf
htop
less
lib32stdc++6
lib32z1
libc6-dev-i386
libc6-i386
libdatetime-perl
liblua5.2-dev
libstring-approx-perl
libstring-shellquote-perl
libtext-glob-perl
libxml-parser-perl
libxml2:i386
libxml2-utils
lua5.2
mysql-client
openjdk-7-jdk
openssh-client
policycoreutils
python3
python-mysqldb
qt5-default
qtchooser
rsync
schedtool
strace
subversion
xclip
xmlstarlet
xorriso
zip
zlib1g-dev
EOF
          ); do
    test -e $x || touch $x
done
echo ok

