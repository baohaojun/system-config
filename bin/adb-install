#!/bin/bash

if [[ "$1" =~ ^/scp: ]]; then
    my-rsync "$1" ~/tmp
    set -- ~/tmp/$(basename "$1")
fi

(
    for x in $(seq 1 60); do
        sleep 1;
        adb_top_activity=$(adb-top-activity)
        if test "$adb_top_activity" =  警告 -o "$adb_top_activity" = "Warning"; then
            adb-tap 747 1186
            break
        fi
    done
) >/dev/null 2>&1 &

the-true-adb install "$@"
