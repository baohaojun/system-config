#!/bin/bash
# this script is for developping lww, use a different HOME for testing!
export OLDHOME=$HOME
export HOME=$1
shift
mkdir -p ~/.logs
mkdir -p ~/.config

if test ~ != $OLDHOME; then
    ln -s $OLDHOME/.config/ibus ~/.config/ -f >/dev/null 2>&1
    cp $OLDHOME/src/github/lww/.gdbinit ~/
fi

"$@"
