#!/bin/bash
sudo bash -c 'echo "deb http://mirrors.aliyun.com/debian/ squeeze main" > /etc/apt/sources.list.d/emdebian.list'
sudo bash -c 'echo "deb http://www.emdebian.org/debian/ squeeze main" >> /etc/apt/sources.list.d/emdebian.list'
sudo apt-get update
apt-cache search armel
sudo apt-get install g++-4.4-arm-linux-gnueabi
