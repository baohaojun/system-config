#!/usr/bin/env bash
set -e
prog1=
prog2=
for p in $(type -a $1 | grep -P '(is|ã¯) /'|pn 3); do
    prog=$(readlink -f "$p")

    if [[ $prog =~ /debug/ ]]; then
        continue;
    elif test "$prog1"; then
        if test "$prog" != "$prog1"; then
            prog2=$prog
            break;
        fi
    else
        prog1=$prog
    fi
done

shift

echo prog1 is $prog1 prog2 is $prog2, "$@" >> ~/.cache/system-config/logs/$(basename $0).log

if test "$prog2"; then
    exec "$prog2" "$@"
else
    die "Can't find 2nd program for $prog1"
fi
