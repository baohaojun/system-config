#!/usr/bin/expect -f

set stty_init -echo
spawn ant $argv
set password ""

expect -re "Please enter.*password.*\n" {
    if [string equal $password ""] {
        set fl [open "|ant-get-keystore-passwd"]
        set password [read $fl]
        close $fl
    }
    send "$password\n";
    exp_continue;
} -re "-release-" {
    exp_continue;
} timeout {
    puts "time out\n"
    exp_continue;
}

exit [lindex [wait] 3]



