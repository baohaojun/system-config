<?xml version='1.0' encoding='utf-8'?>
<!-- filename: sdim.xml -->
<component>
    <name>org.freedesktop.IBus.Sdim</name>
    <description>Sdim Component</description>
    <exec>${libexecdir}/ibus-engine-sdim --ibus</exec>
    <version>@VERSION@</version>
    <author>Haojun Bao &lt;baohaojun@gmail.com&gt;</author>
    <license>LGPL</license>
    <homepage>http://github.com/baohaojun/windows-config/raw/master/</homepage>
    <textdomain>ibus-sdim</textdomain>
    <observed-paths>
        <path>${pkgdatadir}/sdims/</path>
    </observed-paths>
    <engines exec='${libexecdir}/ibus-engine-sdim --xml'/>
</component>
