	email = baohaojun@smartisan.com

[core]
	pager =
	excludesfile = ~/system-config/.git-exclude
[alias]
	st = status
	co = checkout
        dt = difftool -t meld
[difftool "bhj"]
	cmd = args-debug
[guilt]
	diffstat = true
[format]
	pretty = fuller
	signOff = true
[push]
	default = simple
